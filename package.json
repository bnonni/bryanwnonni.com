{
  "name": "bryanwnonni.com",
  "version": "4.1.25",
  "engines": {
    "node": "18.x",
    "yarn": "^1.22.15"
  },
  "dependencies": {
    "@tbd54566975/web5": "^0.7.8",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loading": "^2.0.3",
    "react-scripts": "^5.0.1",
    "smooth-scroll": "^16.1.3",
    "travis": "^0.1.1",
    "yarn": "^1.13.0"
  },
  "scripts": {
    "start": "concurrently --kill-others yarn:prod:*",
    "prod:frontend": "PORT=8080 react-scripts start",
    "prod:backend": "cd bwn-server && npm run server",
    "dev": "concurrently --kill-others yarn:dev:*",
    "dev:frontend": "PORT=8081 react-scripts start",
    "dev:backend": "cd bwn-server && npm run server",
    "build": "concurrently --kill-others yarn:build:*",
    "build:frontend": "react-scripts build",
    "build:backend": "cd bwn-server && npm run server",
    "predeploy": "yarn run build",
    "deploy": "gh pages -d build",
    "test": "react-scripts test",
    "eject": "react-scripts eject"

  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
